  "private": true,
  "description": "基于Vue2 + element-ui 构建的的后台管理系统",
  "scripts": {
    "start": "vue-cli-service serve",
    "dev": "concurrently \"nodemon mock/server.js\" \"vue-cli-service serve\"",
    "build": "vue-cli-service build",
    "mockserver": "nodemon mock/server.js",
    "preview": "node build/index.js --preview",
    "lint": "vue-cli-service lint",
    "lint2": "eslint --ext .js,.vue src",
    "svgo": "svgo -f src/icons/svg --config=src/icons/svgo.yml",
    "new": "plop",
    "lint:vue": "vue-cli-service lint",
    "lint:css": "stylelint --cache src/**/*.{css,scss} --syntax scss --fix",
    "lint:style": "stylelint --cache src/**/*.{html,vue} --custom-syntax postcss-html --fix",
    "prepare": "husky install"
  },
  "keywords": [
    "vue",
    "element-ui"
  ],
  "dependencies": {
    "axios": "^0.22.0",
    "element-ui": "^2.15.6",
    "normalize.css": "7.0.0",
    "nprogress": "0.2.0",
    "vue": "^2.6.11",
    "vue-router": "3.0.2",
    "vuex": "3.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.0.0",
    "@babel/register": "7.0.0",
    "@vue/cli-plugin-babel": "3.5.3",
    "@vue/cli-plugin-eslint": "^4.5.13",
    "@vue/cli-service": "^4.5.13",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "23.6.0",
    "chalk": "2.4.2",
    "chokidar": "2.1.5",
    "concurrently": "^5.0.2",
    "connect": "3.6.6",
    "cookie-parser": "^1.4.5",
    "dart-sass": "^1.25.0",
    "debug": "^4.3.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^7.19.1",
    "express": "^4.17.1",
    "hard-source-webpack-plugin": "^0.13.1",
    "html-webpack-plugin": "3.2.0",
    "http-errors": "^1.8.0",
    "husky": "^7.0.2",
    "lint-staged": "8.1.5",
    "mockjs": "^1.1.0",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.13",
    "plop": "2.3.0",
    "prettier": "^2.4.1",
    "runjs": "^4.3.2",
    "sass": "1.32.*",
    "sass-loader": "^7.1.0",
    "script-ext-html-webpack-plugin": "2.1.3",
    "script-loader": "^0.7.2",
    "serve-static": "^1.13.2",
    "simple-progress-webpack-plugin": "^2.0.0",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-order": "^4.1.0",
    "svg-sprite-loader": "4.1.3",
    "svgo": "1.2.0",
    "vue-template-compiler": "^2.6.14",
    "worker-loader": "^3.0.8"
  },
  "engines": {
    "node": ">=8.9",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 9"
  ]
